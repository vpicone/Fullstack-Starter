{
    "extends": "eslint:recommended",
    "plugins": [
        "prettier"
    ],
    "env": {
        "es6": true,
        "node": true
    },
    "rules": {
        "prettier/prettier": [
            "error",
            {
                "trailingComma": "es5",
                "singleQuote": true,
                "printWidth": 100
            }
        ]
    }
}